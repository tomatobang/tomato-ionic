  "private": true,
  "scripts": {
    "dev": "bnr dev",
    "prod": "bnr prod",
    "clean": "ionic-app-scripts clean",
    "build": "ionic-app-scripts build",
    "build:android": "ionic-app-scripts build --prod --platform android --target cordova",
    "build:ios": "ionic-app-scripts build --prod --platform ios --target cordova",
    "ionic:serve": "ionic-app-scripts serve",
    "test": "karma start ./test-config/karma.conf.js",
    "test-ci": "karma start ./test-config/karma.conf.js --single-run",
    "test-coverage": "karma start ./test-config/karma.conf.js --coverage",
    "e2e": "npm run e2e-update && npm run e2e-test",
    "e2e-test": "protractor ./test-config/protractor.conf.js",
    "e2e-update": "webdriver-manager update --standalone false --gecko false",
    "e2e-update-local": "./node_modules/protractor/bin/webdriver-manager update",
    "lint": "tslint  -p tsconfig.json -c tslint.json --fix",
    "scss-lint": "scss-lint",
    "commitmsg": "commitlint -e",
    "release": "standard-version",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test-ci",
      "commit-msg": "commitlint -e"
    }
  },
  "betterScripts": {
    "dev": {
      "command": "ionic-app-scripts serve",
      "env": {
        "NODE_ENV": "dev",
        "PLATFORM": "web"
      }
    },
    "prod": {
      "command": "ionic-app-scripts serve",
      "env": {
        "NODE_ENV": "prod",
        "PLATFORM": "web"
      }
    }
  },
  "config": {
    "ionic_webpack": "./config/webpack.config.js",
